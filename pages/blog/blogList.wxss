/* pages/myOrder/myOrder.wxss */
.nearCard-fl {
  width: 40%;
  float: left;
}

.nearCard-fl image {
  width: 200rpx;
  height: 200rpx;
  padding: 8% 15%;
}

.nearCard-fr {
  font-size: 30rpx;
  line-height: 50rpx;
  width: 60%;
  margin-left: 40%;
  margin-top: 15rpx;
  color: #999;
}

.nearCard-fr {
  font-size: 26rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  align-content: center;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.title {
  font-size: 26rpx;
  overflow: hidden;
  white-space:nowrap;
  word-break: break-all;
  align-content: center;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
.content {
  font-size: 26rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  align-content: center;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.down {
  background-color: #efefef;
  border-bottom-left-radius: 20px;
  border-bottom-right-radius: 20rpx;
}

.down view {
  font-size: 26rpx;
  color: #999;
  line-height: 50rpx;
}

.feedBack {
  text-align: center;
  background-color: #eecfbd;
  border-bottom-left-radius: 10px;
  border-bottom-right-radius: 10rpx;
  padding: 5rpx 0;
  margin: 10rpx 0 0 0;
}